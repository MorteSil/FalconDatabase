<?xml version="1.0" encoding="utf-8"?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:simpleType name="floatThreePrec">
		<xs:restriction base="xs:decimal">
			<xs:fractionDigits value="3" />
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="FCDRecords">
		<xs:complexType>
			<xs:sequence>
				<xs:element maxOccurs="unbounded" name="FCD">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="CtIdx" type="xs:short" />
							<xs:element name="RepairTime" type="xs:short" />
							<xs:element name="DisplayPriority" type="xs:unsignedByte" />
							<xs:element name="Flags" type="xs:unsignedShort" />
							<xs:element name="Name" type="xs:string" default =" " />
							<xs:element name="HitPoints" type="xs:short" />
							<xs:element name="RampHeight" type="xs:short" />
							<xs:element name="RampAngle" type="floatThreePrec" />
							<xs:element name="RadarIdx" type="xs:short" />
							<xs:element name="Det_NoMove" type="floatThreePrec" />
							<xs:element name="Det_Foot" type="floatThreePrec" />
							<xs:element name="Det_Wheeled" type="floatThreePrec" />
							<xs:element name="Det_Tracked" type="floatThreePrec" />
							<xs:element name="Det_LowAir" type="floatThreePrec" />
							<xs:element name="Det_Air" type="floatThreePrec" />
							<xs:element name="Det_Naval" type="floatThreePrec" />
							<xs:element name="Det_Rail" type="floatThreePrec" />
							<xs:element name="Dam_None" type="xs:unsignedByte" />
							<xs:element name="Dam_Penetration" type="xs:unsignedByte" />
							<xs:element name="Dam_HighExplosive" type="xs:unsignedByte" />
							<xs:element name="Dam_Heave" type="xs:unsignedByte" />
							<xs:element name="Dam_Incendairy" type="xs:unsignedByte" />
							<xs:element name="Dam_Proximity" type="xs:unsignedByte" />
							<xs:element name="Dam_Kinetic" type="xs:unsignedByte" />
							<xs:element name="Dam_Hydrostatic" type="xs:unsignedByte" />
							<xs:element name="Dam_Chemical" type="xs:unsignedByte" />
							<xs:element name="Dam_Nuclear" type="xs:unsignedByte" />
							<xs:element name="Dam_Other" type="xs:unsignedByte" />
						</xs:sequence>
						<xs:attribute name="Num" type="xs:int" use="required" />
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>